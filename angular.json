{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/host",
      "sourceRoot": "projects/host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/host",
            "index": "projects/host/src/index.html",
            "main": "projects/host/src/main.ts",
            "polyfills": "projects/host/src/polyfills.ts",
            "tsConfig": "projects/host/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/host/src/favicon.ico",
              "projects/host/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/host/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/host/src/environments/environment.ts",
                  "with": "projects/host/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "host:build:production"
            },
            "development": {
              "browserTarget": "host:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "host:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/host/src/test.ts",
            "polyfills": "projects/host/src/polyfills.ts",
            "tsConfig": "projects/host/tsconfig.spec.json",
            "karmaConfig": "projects/host/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/host/src/favicon.ico",
              "projects/host/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/host/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "mfe1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/mfe1",
      "sourceRoot": "projects/mfe1/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mfe1",
            "index": "projects/mfe1/src/index.html",
            "main": "projects/mfe1/src/main.ts",
            "polyfills": "projects/mfe1/src/polyfills.ts",
            "tsConfig": "projects/mfe1/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/mfe1/src/favicon.ico",
              "projects/mfe1/src/assets"
            ],
            "styles": [
              "projects/mfe1/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/mfe1/src/environments/environment.ts",
                  "with": "projects/mfe1/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "mfe1:build:production"
            },
            "development": {
              "browserTarget": "mfe1:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mfe1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mfe1/src/test.ts",
            "polyfills": "projects/mfe1/src/polyfills.ts",
            "tsConfig": "projects/mfe1/tsconfig.spec.json",
            "karmaConfig": "projects/mfe1/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/mfe1/src/favicon.ico",
              "projects/mfe1/src/assets"
            ],
            "styles": [
              "projects/mfe1/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "lib-log-message-shared": {
      "projectType": "library",
      "root": "projects/lib-log-message-shared",
      "sourceRoot": "projects/lib-log-message-shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/lib-log-message-shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/lib-log-message-shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/lib-log-message-shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lib-log-message-shared/src/test.ts",
            "tsConfig": "projects/lib-log-message-shared/tsconfig.spec.json",
            "karmaConfig": "projects/lib-log-message-shared/karma.conf.js"
          }
        }
      }
    },
    "log-message-tester": {
      "projectType": "library",
      "root": "projects/log-message-tester",
      "sourceRoot": "projects/log-message-tester/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/log-message-tester/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/log-message-tester/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/log-message-tester/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/log-message-tester/src/test.ts",
            "tsConfig": "projects/log-message-tester/tsconfig.spec.json",
            "karmaConfig": "projects/log-message-tester/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "host"
}
